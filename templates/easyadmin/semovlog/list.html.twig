{% extends 'easyadmin/default/list.html.twig' %}
{% block content_header %}
    {{ parent() }}
    <div class="row">
        <div class="col-sm-12">
            <h4>Filtrar por fecha</h4>
        </div>
        <div class="col-sm-12">
            <div class="form-group custom-datetime col-sm-4">
                <label>Fecha Inicio</label>
                <input type="text" class="form-control" />
            </div>
            <div class="form-group custom-datetime col-sm-4">
                <label>Fecha Fin</label>
                <input type="text" class="form-control" />
            </div>
            <div class="form-group col-sm-2">
                <button class="btn btn-primary"><i class="fa fa-recycle"></i> Filtrar</button>
            </div>
        </div>
    </div>
{% endblock %}